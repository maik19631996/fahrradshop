<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_LEFT" depthTest="DISABLE" disable="false" fillWidth="true" focusTraversable="false" prefHeight="500.0" prefWidth="600.0" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.example.bikeshopmodular.PrimaryController">
  <children>
    <Label text="Fahrradshop" textAlignment="LEFT" wrapText="false">
      <font>
        <Font name="System Bold Italic" size="24.0" />
      </font>
    </Label>
    <Label text="Rahmentyp" textAlignment="LEFT" wrapText="false" />
    <ChoiceBox id="Rahmen" fx:id="choiceBoxRahmen" value="Item 1">
      <items>
        <FXCollections fx:factory="observableArrayList">
          <String fx:value="Item 1" />
          <String fx:value="Item 2" />
          <String fx:value="Item 3" />
        </FXCollections>
      </items>
    </ChoiceBox>
    <Label text="Rahmenfarbe" textAlignment="LEFT" wrapText="false" />
    <ChoiceBox id="choiceBoxRahmen" fx:id="choiceBoxRahmenFarbe" prefWidth="67.0">
      <items>
        <FXCollections fx:factory="observableArrayList" />
      </items>
    </ChoiceBox>
    <Label text="Reifen" textAlignment="LEFT" wrapText="false" />
    <ChoiceBox id="getReifen" fx:id="choiceBoxReifen" prefWidth="67.0">
      <items>
        <FXCollections fx:factory="observableArrayList" />
      </items>
    </ChoiceBox>
    <Label text="Antrieb" textAlignment="LEFT" wrapText="false" />
    <ChoiceBox id="getAntriebe" fx:id="choiceBoxAntriebe" maxHeight="-1.0" minWidth="-1.0" prefWidth="67.0">
      <items>
        <FXCollections fx:factory="observableArrayList" />
      </items>
    </ChoiceBox>
    <Label text="Deine Schrittlänge" textAlignment="LEFT" wrapText="false" />
    <TextField fx:id="textFieldSchritt" maxWidth="75.0" prefHeight="-1.0" prefWidth="50.0" />
    <Label text="Deine Körpergröße" textAlignment="LEFT" wrapText="false" />
    <TextField id="getGroesse" fx:id="textFieldGroesse" maxWidth="75.0" minWidth="-1.0" prefWidth="75.0" />
    <Button id="primaryButton" alignment="CENTER" defaultButton="true" minHeight="-1.0" minWidth="-1.0" onAction="#switchToSecondary" text="switch" VBox.vgrow="ALWAYS">
      <VBox.margin>
        <Insets left="450.0" />
      </VBox.margin>
    </Button>
  </children>
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
</VBox>
